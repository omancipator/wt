<h1>The Home View!</h1>

<table>
  <thead>
    <tr>
      <th>Comment</th>
      <th>Weight</th>
      <th>Fat per</th>
      <th>Bmi</th>
      <th>Weight date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr ng-repeat="weight_post in weight_posts">
        <td>{{weight_post.comment}}</td>
        <td>{{weight_post.weight}}</td>
        <td>{{weight_post.fat_per}}</td>
        <td>{{weight_post.bmi}}</td>
        <td>{{weight_post.weight_date | date: "EEE MM/dd/yyyy"}}</td>

      </tr>   
  </tbody>
</table>
  
  
